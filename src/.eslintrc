{
    "extends": [
      "react-app",
      "airbnb",
      "airbnb/hooks",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": ["jsx-a11y"],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "linebreak-style": 0,
      "comma-dangle": ["error", "never"],
      "react/prop-types": 0
    }
  }
  